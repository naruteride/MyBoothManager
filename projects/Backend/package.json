{
  "name": "@myboothmanager/backend",
  "version": "0.0.0",
  "author": "somni <me@somni.one>",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production yarn migrate:prod up && node dist/src/main",
    "start:prod:no-migrate": "NODE_ENV=production node dist/src/main",
    "migrate": "NODE_ENV=development ts-node -r tsconfig-paths/register scripts/db-migrator/migrate",
    "migrate:prod": "NODE_ENV=production node dist/scripts/db-migrator/migrate",
    "seed": "NODE_ENV=development ts-node -r tsconfig-paths/register scripts/db-migrator/seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^7.0.3",
    "@myboothmanager/common": "../../packages/Common",
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-fastify": "^10.3.7",
    "app-root-path": "^3.1.0",
    "argon2": "^0.40.1",
    "chalk": "^4",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "mysql2": "^3.9.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.33.3",
    "umzug": "^3.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "expect-type": "^0.19.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "fastify": "4.26.0"
  }
}
