{
  "name": "@myboothmanager/backend",
  "version": "0.0.0",
  "author": "somni <me@somni.one>",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production yarn migrate:prod up && node dist/src/main",
    "start:prod:no-migrate": "NODE_ENV=production node dist/src/main",
    "migrate": "NODE_ENV=development ts-node -r tsconfig-paths/register scripts/db-migrator/migrate",
    "migrate:prod": "NODE_ENV=production node dist/scripts/db-migrator/migrate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@myboothmanager/common": "../../packages/Common",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-fastify": "^10.2.10",
    "app-root-path": "^3.1.0",
    "argon2": "=0.31.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.0",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/sequelize": "^4.28.19",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "expect-type": "^0.17.3",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "fastify": "4.25.0"
  }
}
