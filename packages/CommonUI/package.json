{
  "name": "@myboothmanager/common-ui",
  "version": "0.0.0",
  "author": "somni <me@somni.one>",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "scripts": {
    "build": "yarn clean && run-p type-check \"build-only {@}\" --",
    "clean": "rimraf ./dist",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "files": [
    "./dist"
  ],
  "types": "./dist/common-ui.d.ts",
  "module": "./dist/common-ui.mjs",
  "devDependencies": {
    "@myboothmanager/common": "../Common",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "npm-run-all2": "^6.1.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^4.5.1",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-tsconfig-paths": "^4.2.2",
    "vue": "^3.3.11",
    "vue-facing-decorator": "^3.0.4",
    "vue-tsc": "^1.8.25",
    "vuetify": "^3.4.6"
  },
  "peerDependencies": {
    "@mdi/font": ">=7",
    "@myboothmanager/common": "*",
    "vue": ">=3",
    "vue-facing-decorator": ">=3",
    "vuetify": ">=3"
  }
}
